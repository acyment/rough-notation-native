{
  "name": "rough-notation-native",
  "version": "0.1.0",
  "description": "Create and animate hand-drawn annotations in React Native/Expo apps",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "prepublishOnly": "npm run build",
    "lint": "tslint -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "./scripts/run-example.sh",
    "example:install": "cd example && bun install",
    "example:start": "npm run build && cd example && bun run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acyment/rough-notation-native.git"
  },
  "keywords": [
    "annotate",
    "rough",
    "sketchy",
    "react-native",
    "expo"
  ],
  "author": "Preet Shihn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acyment/rough-notation-native/issues"
  },
  "homepage": "https://github.com/acyment/rough-notation-native#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "react-native-svg": ">=13.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "typescript": "^5.0.0"
  }
}
